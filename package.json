{
  "name": "jimnieters-online",
  "private": true,
  "description": "Online Portfolio of Jim Nieters, a ux leader who loves to inspire and be inspired by design. Great design disrupts markets and delights users.",
  "version": "0.0.3",
  "author": "Devanand <dev.ryder88@gmail.com>",
  "dependencies": {
    "gatsby": "^2.10.4",
    "gatsby-background-image": "^0.7.4",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "^2.2.0",
    "gatsby-plugin-netlify-cms": "^4.1.1",
    "gatsby-plugin-offline": "^2.2.0",
    "gatsby-plugin-postcss": "^2.1.0",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-robots-txt": "^1.4.0",
    "gatsby-plugin-use-dark-mode": "^1.1.2",
    "glhd-tailwindcss-transitions": "^0.3.0",
    "lodash": "^4.17.11",
    "netlify-cms-app": "^2.9.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "tailwindcss-dark-mode": "^1.0.3",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "gatsby-image": "^2.2.3",
    "gatsby-plugin-sharp": "^2.2.1",
    "gatsby-source-filesystem": "^2.1.1",
    "gatsby-transformer-json": "^2.2.0",
    "gatsby-transformer-sharp": "^2.2.0",
    "generate-changelog": "^1.8.0",
    "prettier": "^1.18.2",
    "release-it": "^12.3.0",
    "tailwindcss": "^1.0.4"
  },
  "keywords": [
    "portfolio",
    "online",
    "jimnieters",
    "designer",
    "blog"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "release-patch": "changelog -p && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it patch",
    "release-minor": "changelog -m && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it minor",
    "release-major": "changelog -M && git add CHANGELOG.md && git commit -m 'chore:Update CHANGELOG.md' && release-it major"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "release": {
    "repositoryUrl": "https://github.com/devanandb/jimnieters-online",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  }
}
